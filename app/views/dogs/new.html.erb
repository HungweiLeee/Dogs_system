<%= form_for @dog , :url => {:controller => "dogs" , :action => "create"} do |f| %>
	
	<%= f.label :name , "name"%>
	<%= f.text_field :name %>

	<%= f.label :descrip , "descrip"%>
	<%= f.text_field :descrip %>

    <%= f.label :date %>
	<%= f.date_select :date %>

	<%= f.submit "create" %>

<% end %>