<h1> List </h1>

<ul>
<% @dogs.each do |d| %>
  
 	  <li><%= d.name %></li>
 	  <%= d.descrip %>
	  <%= link_to 'show' , dog_path(d) %>
	  <%= link_to 'edit' , edit_dog_path(d) %>
	  <%= button_to 'delete', dog_path(d), :method => :delete, :data => { :confirm => "Are you sure?" } %>
<% end %>
</ul>

<%= link_to 'New Event', dogs_url %>
